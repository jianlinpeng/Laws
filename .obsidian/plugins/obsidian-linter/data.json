  "customRegexes": [
    {
      "label": "法条-法条索引号（需要js）设计",
      "find": "\\n\\*\\*第(\\d+|[零一二三四五六七八九十百]+)条\\*\\*",
      "replace": "",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "法律-添加双链",
      "find": "\\|(\\s*)(\\S+)(\\s*\\|\\s*)(\\d{4}\\-)",
      "replace": "|$1[[$2]]$3$4",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "法律-清除外部链接",
      "find": "\\[(.*?)\\]\\(.*?\\)",
      "replace": "$1",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "法律-第x编缩进",
      "find": "\\n第(\\d+|[零一二三四五六七八九十百]+)编\\s*",
      "replace": "\\n# 第$1编 ",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "附则",
      "find": "\\n附则",
      "replace": "\\n\\n## 附则 ",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "法律-第x分编缩进",
      "find": "\\n第(\\d+|[零一二三四五六七八九十百]+)分编\\s*",
      "replace": "\\n## 第$1分编 ",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "法律-第x部分缩进",
      "find": "\\n第(\\d+|[零一二三四五六七八九十百]+)部分\\s*",
      "replace": "\\n## 第$1部分 ",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "法律-第x章缩进",
      "find": "\\n第(\\d+|[零一二三四五六七八九十百]+)章\\s*",
      "replace": "\\n### 第$1章 ",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "法律-第x节缩进",
      "find": "\\n第(\\d+|[零一二三四五六七八九十百]+)节\\s*",
      "replace": "\\n#### 第$1节 ",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "法律-第x条缩进",
      "find": "\\n第(\\d+|[零一二三四五六七八九十百千]+)条\\s*",
      "replace": "\\n**第$1条** ",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "法条尾部索引",
      "find": "\\s*\\^t\\s(\\d{1,4})\\s",
      "replace": " ^t$1\\n",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "有序列表替换（终）",
      "find": "\\n\\s*(\\d{1,3})\\s*[\\.．]\\s*",
      "replace": "\\n\\n$1 . ",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "中文序号替换-1",
      "find": "\\n([零一二三四五六七八九十百]+)　、",
      "replace": "\\n$1、",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "有序列表复原",
      "find": "\\n\\s*(\\d{1,3})\\s*[\\.．]\\s*",
      "replace": "\\n$1. ",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "中文序号标题替换",
      "find": "\\n([零一二三四五六七八九十百]+)、",
      "replace": "\\n### $1、",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "法律-名称",
      "find": "(\\d+|[零一二三四五六七八九十百]+、.*)\\n\\n（(\\d+|[零一二三四五六七八九十百]+)）\\s*(.*)n\\n(.*)法",
      "replace": "$1\\n\\n### （$2）$3\\n\\n$4",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "法律-首括号改变",
      "find": "\\n\\((.{1,3})\\)\\s*",
      "replace": "（$1）",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "文本内括号改变",
      "find": "\\n\\s*\\((.*)\\)\\s*",
      "replace": "\\n\\n（$1）",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "法律-尾空格缩减",
      "find": "\\n#### 第(\\d+|[零一二三四五六七八九十百]+)章\\s*",
      "replace": "\\n#### 第$1章 ",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "真题-去除选择题空行",
      "find": "\\n{1,3}([ABCD])\\.",
      "replace": "\\n$1.",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "标签生成1",
      "find": "\\n\\s*-\\s*",
      "replace": "/",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "标签生成2:——还原tags:/",
      "find": "tags:/",
      "replace": "tags:\\n  - ",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "标签生成3:——还原cssclasses",
      "find": "cssclasses:/serif/index",
      "replace": "cssclasses:\\n  - serif\\n  - index",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "标签生成4:——还原/--",
      "find": "/--",
      "replace": "\\n---",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "题目拆分",
      "find": "(\\d{1,3})\\.\\s",
      "replace": "### $1. 题目\\n\\n",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "题目=>有序列表",
      "find": "## (\\d{1,2})\\.\\n{1,2}",
      "replace": "$1. ",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "引用格式化",
      "find": "\\^t\\s(\\d{1,4})",
      "replace": "^t$1",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "冒号:替换",
      "find": ":",
      "replace": "：",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": ",替换",
      "find": ",",
      "replace": "，",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "指导案例格式化 0",
      "find": "\\n关键词",
      "replace": "\\n### 【关键词】\\n\\n",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "指导案例格式化 1",
      "find": "\\n裁判要点",
      "replace": "\\n### 【裁判要点】",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "指导案例格式化 2",
      "find": "\\n相关法条",
      "replace": "\\n### 【相关法条】",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "指导案例格式化 3",
      "find": "\\n基本案情",
      "replace": "\\n### 【基本案情】",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "指导案例格式化 4",
      "find": "\\n裁判结果",
      "replace": "\\n### 【裁判结果】",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "指导案例格式化 5",
      "find": "\\n裁判理由",
      "replace": "\\n### 【裁判理由】",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "&lt；",
      "find": "&lt；",
      "replace": "<",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "&gt；",
      "find": "&gt；",
      "replace": ">",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "◆典型案例",
      "find": "\\n{0,2}◆典型案例\\s*",
      "replace": "",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "一级",
      "find": "\\n(\\d{1,3}) ",
      "replace": "# $1 ",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "二级",
      "find": "\\n(\\d{1,3})\\.(\\d{1,3}) ",
      "replace": "## $1.$2 ",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "三级",
      "find": "\\n(\\d{1,3})\\.(\\d{1,3})\\.(\\d{1.3}) ",
      "replace": "### $1.$2.$3 ",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "css代码格式化",
      "find": "cssclasses: serif index",
      "replace": "cssclasses:\\n  - serif\\n  - index",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "首航去除空格",
      "find": "\\n\\n　　",
      "replace": "\\n\\n",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "标题修改",
      "find": " # ",
      "replace": "",
      "flags": "gm",
      "enabled": false
    },
    {
