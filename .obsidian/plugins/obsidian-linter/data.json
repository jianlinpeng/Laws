      "flags": "gm",
      "enabled": false
    },
    {
      "label": "案例-典型意义",
      "find": "（一）典型意义",
      "replace": "### （一）典型意义",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "案例-基本案情",
      "find": "（二）基本案情",
      "replace": "### （二）基本案情",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "案例-裁判结果",
      "find": "（三）裁判结果",
      "replace": "### （三）裁判结果",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "案例-民法典条文指引",
      "find": "（四）民法典条文指引",
      "replace": "### （四）民法典条文指引",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "首航锁紧？",
      "find": "\\n　　",
      "replace": "\\n",
      "flags": "gm",
      "enabled": true
    },
    {
